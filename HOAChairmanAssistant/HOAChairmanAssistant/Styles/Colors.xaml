<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="ForegroundColorLight" Color="#FFFFFF" />
    <!--Background Color -->
    <SolidColorBrush x:Key="BackgroundDarkBlueBrush" Color="#607D8B" />
    <!--Second Backgound Color -->
    <SolidColorBrush x:Key="BackgroundLightBlueBrush" Color="#009688" />
    <!--Button MaterialDesign Color-->
    <SolidColorBrush x:Key="PrimaryHueMidBrush" Color="#607D8B" />

    <!--Primary-->
    <SolidColorBrush x:Key="PrimaryHueLightBrush" Color="#607D8B" />
    <SolidColorBrush x:Key="PrimaryHueLightForegroundBrush" Color="#FF333333" />

    <SolidColorBrush x:Key="PrimaryHueMidForegroundBrush" Color="#FFDDDDDD" />

    <SolidColorBrush x:Key="PrimaryHueDarkBrush" Color="#009688" />
    <SolidColorBrush x:Key="PrimaryHueDarkForegroundBrush" Color="#FFFFFFFF" />
    <!--Accent-->
    <SolidColorBrush x:Key="SecondaryAccentBrush" Color="#009688" />
    <SolidColorBrush x:Key="SecondaryAccentForegroundBrush" Color="#FFFFFF" />
</ResourceDictionary>